{
  "name": "financemate-ag-ui",
  "version": "1.0.0",
  "description": "AG-UI integration for FinanceMate - Advanced multi-agent coordination with tiered business model",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "start": "node copilotkit-ui/server/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon copilotkit-ui/server/index.js",
    "client:dev": "cd copilotkit-ui/client && vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.3",
    "@types/ws": "^8.5.5",
    "@vitejs/plugin-react": "^4.0.4",
    "concurrently": "^8.2.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "terser": "^5.40.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "keywords": [
    "ag-ui",
    "multi-agent",
    "finance",
    "ai",
    "langraph",
    "apple-silicon"
  ],
